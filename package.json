{
  "name": "fease",
  "version": "0.2.0",
  "license": "mit",
  "private": false,
  "description": "A functional programming library for creating composable easing functions for animation.",
  "author": "Mims H. Wright",
  "homepage": "https://mimshwright.github.io/fease/",
  "repository": {
    "type": "git",
    "url": "https://github.com/mimshwright/fease.git"
  },
  "bugs": {
    "url": "https://github.com/mimshwright/fease/issues"
  },
  "keywords": [
    "animation",
    "easing",
    "functional-programming",
    "pure-function",
    "lerp",
    "graph"
  ],
  "type": "module",
  "main": "./dist/fease.umd.cjs",
  "module": "./dist/fease.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/fease.js",
      "require": "./dist/fease.umd.cjs"
    }
  },
  "files": [
    "dist",
    "demo"
  ],
  "scripts": {
    "prepare:peers": "install-peers",
    "prepare:husky": "husky install",
    "prepare": "run-p prepare:*",
    "dev": "vite",
    "start": "run-s dev",
    "preview": "vite preview",
    "test": "vitest --watch false",
    "test:watch": "vitest",
    "test:coverage": "run-s 'test -- --coverage'",
    "test:ui": "run-s 'test -- --ui'",
    "lint:typescript": "run-s build:tsc",
    "lint:prettier": "prettier -l .",
    "lint:eslint": "eslint *.{t,j,cj}s{,x} --no-error-on-unmatched-pattern",
    "lint": "run-p lint:*",
    "fix:prettier": "run-s 'lint:prettier -- -w'",
    "fix:eslint": "run-s 'lint:eslint -- --fix'",
    "fix": "run-s fix:*",
    "build:tsc": "tsc",
    "build:vite": "vite build",
    "build:vite:demo": "run-s 'build:vite -- -c vite.config.demo.ts'",
    "build": "run-s build:tsc build:vite",
    "build:demo": "run-s build:tsc build:vite:demo",
    "commit": "cz",
    "ci": "run-s commit",
    "release": "standard-version",
    "pushpub": "git push --follow-tags origin main && yarn publish",
    "predeploy": "run-s build:demo",
    "deploy": "gh-pages -d demo"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/im-fell-french-canon": "^4.5.8",
    "@fontsource/yatra-one": "^4.5.10",
    "@inlet/react-pixi": "^6.8.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.10.17",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/ramda": "^0.28.20",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-highlight": "^0.12.5",
    "@types/react-visibility-sensor": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.26.0",
    "@vitest/ui": "^0.26.1",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-typescript": "^0.14.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.2",
    "install-peers-cli": "^2.2.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "pixi-easing-graph": "^1.1.0",
    "pixi.js": "^7.0.4",
    "prettier": "^2.8.0",
    "react": "^17",
    "react-dom": "^17",
    "react-highlight": "^0.15.0",
    "react-visibility-sensor": "^5.1.1",
    "standard-version": "^9.5.0",
    "typescript": "*",
    "vite": "^3.2.4",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.26.1"
  },
  "dependencies": {
    "ramda": "^0.28.0"
  }
}
